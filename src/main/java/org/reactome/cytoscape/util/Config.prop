# Configuration information
# Control if a local service should be used
networkService=org.reactome.cytoscape.service.LocalService
# The following property is not supported any more. A simple dialog is used.
# dataSourceURL=http://reactomedev.oicr.on.ca/cgi-bin/eventbrowser?DB=${DB_NAME}&FORMAT=sectioned&ID=
moduleColors=0,0,153;51,153,0;153,0,153;102,102,0;0,102,102;204,153,0;204,0,102;204,204,255;0,102,51;0,204,204;102,0,51;0,51,51;0,102,153;255,102,255;255,255,102
# As of July, 2018, changed to the main Reactome web site.
userGuideURL=https://www.reactome.org/tools/reactome-fiviz
# A list of FI network versions available for selection
FINetworkVersions=2021,2020,2019
# For local test
#2021_restfulURL=http://localhost:8080/corews/FIService/
#2020_restfulURL=http://localhost:8080/corews/FIService/
#2019_restfulURL=http://localhost:8080/corews/FIService/
# For new cpws at Amazon AWS
2021_restfulURL=http://cpws.reactome.org/caBigR3WebApp2021/FIService/
2020_restfulURL=http://cpws.reactome.org/caBigR3WebApp2020/FIService/
2019_restfulURL=http://cpws.reactome.org/caBigR3WebApp2019_mm/FIService/
# Reactome stuff
#ReactomeRESTfulAPI=http://localhost:8080/corews/FIService/ReactomeRestful/
ReactomeRESTfulAPI=http://cpws.reactome.org/caBigR3WebApp2021/FIService/ReactomeRestful/
# A list of evidences and their labels
evidenceProperties=humanInteraction,scePPI,dmePPI,celPPI,mousePPI,genewaysPPI,pavlidisGeneExp,carlosGeneExp,pfamDomainInt,goBPSharing,probability
evidenceNames=Human Interaction,Yeast Interaction,Fly Interaction,Worm Interaction,Mouse Interaction,Geneways Interaction,Pavlidis Gene Exp,Carlos Gene Exp,PfamDomain Interaction,GO BP Sharing,Score
#ReactomeURL=http://www.reactome.org/cgi-bin/eventbrowser?DB=gk_current&ID=
# Use the detail view for robust. For mouse instances, pathwaybrowser cannot work for DB_ID.
ReactomeURL=https://www.reactome.org/content/detail/
# Use to control the debug model for PGM analysis
PGMDebug=false
# The file has pre-generated factor graph objects in a zipped XML file
factorGraphFiles=PathwayDiagramsFactorGraphs.xml.zip
fgEscapeNames=ATP, ADP, Pi, H2O, GTP, GDP, CO2, H+
# A flag to control if cancer targetome should be enabled. This flag will be removed so that enabling is the default after the manuscript is accepted
enableCancerTargetome=true
# A flag to control if mechismo data visualization should be enabled
enableMechismo=true
# Serve contents via WS
MechismoWSURL=http://cpws.reactome.org/mechismows/
#MechismoWSURL=http://localhost:8070/
# Production server
gseaWSURL=http://cpws.reactome.org/gsea-service/
#gseaWSURL=http://localhost:8282/